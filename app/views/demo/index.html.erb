        reorderColumns: true,
        columns: [
			{ field: 'recid', caption: 'Rec. Id', size: '200px', resizable: true, hidden: true },
			{ field: 'id', caption: 'ID', size: '200px', resizable: true },
			{ field: 'submit_date', caption: 'Submit Date', size: '200px', resizable: true },
			{ field: 'project_name', caption: 'Project Name', size: '200px', resizable: true },
			{ field: 'trimming', caption: 'Trimming', size: '200px', resizable: true },
			{ field: 'assembler', caption: 'Assembler', size: '200px', resizable: true },
			{ field: 'depth_filter', caption: 'Depth filter', size: '100px', resizable: true },
			{ field: 'collection_date', caption: 'Collection date', size: '200px', resizable: true },
			{ field: 'organism', caption: 'Organism', size: '200px', resizable: true },
			{ field: 'host', caption: 'Host', size: '200px', resizable: true },
			{ field: 'place', caption: 'Place', size: '200px', resizable: true },
