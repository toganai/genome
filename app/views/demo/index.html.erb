				total_base: '1,995',
				n50: '19,225',
				number_of_genes: '31',
				comment: 'MRY=15-412, size=23Kbp,',
				inc_type: 'R',
				ar_genes: 'beta-lactamase (id: 100, cov: 94) fluroquinolone acetylating aminoglycoside acetyltransferase (id: 100, cov: 100)'
