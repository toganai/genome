			{ field: 'contiglength', caption: 'contiglength(bp)', size: '200px', resizable: true },
			{ field: 'total_base', caption: 'Total base(bp)', size: '200px', resizable: true },
			{ field: 'n50', caption: 'N50 (bp)', size: '200px', resizable: true },
			{ field: 'number_of_genes', caption: 'Number of genes', size: '200px', resizable: true },
			{ field: 'comment', caption: 'Comment', size: '200px', resizable: true },
			{ field: 'inc_type', caption: 'Inc type', size: '200px', resizable: true },
			{ field: 'ar_genes', caption: 'AR genes', size: '200px', resizable: true }
        ],
        records: [
            {
				recid: 1,
				id: 'ID=20xxxxxxxxxxxxxxx',
