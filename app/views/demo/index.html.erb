    <script type="text/javascript" src="http://w2ui.com/src/w2ui-1.4.2.min.js"></script>
<script type="text/javascript">
$(function () {
	$().w2destroy('grid');
    $('#grid').w2grid({ 
        name: 'grid',
        show: {
            footer: true,
            lineNumbers : true,
            selectColumn: true
